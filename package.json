{
	"name": "github-pages",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack -p 3001",
		"buildStart": "npm run build && npm run start",
		"build": "next build",
		"start": "next start -p 3001",
		"lint": "npx @biomejs/biome lint --write",
