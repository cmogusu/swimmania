{
	"name": "github-pages",
	"version": "0.1.0",
	"private": true,
	"description": "App that lists the everything related to swimming",
	"keywords": [
		"swimming",
		"coahes",
		"lifeguards",
		"pools"
	],
	"author": {
		"name": "Clive Mogusu",
		"email": "cmogusu@gmail.com"
	},
	"engines": {
		"node": ">=22.5"
	},
	"scripts": {
		"dev": "next dev --turbopack -p 3001",
		"buildStart": "npm run build && npm run start",
		"build": "next build",
		"start": "next start -p 3001",
		"test": "vitest",
		"lint": "npx @biomejs/biome lint --write",
		"db:up": "docker-compose -f docker-compose.db.yml up -d",
		"db:recreate": "docker-compose -f docker-compose.db.yml up -d --force-recreate --remove-orphans",
		"db:down": "docker-compose -f docker-compose.db.yml down",
		"migrate": "pnpm exec prisma migrate dev",
		"generate": "pnpm exec prisma generate"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^2.11.1",
		"@auth/unstorage-adapter": "^2.10.0",
		"@googlemaps/extended-component-library": "^0.6.14",
		"@googlemaps/js-api-loader": "^2.0.2",
		"@prisma/adapter-mariadb": "^6.18.0",
		"@prisma/client": "^6.18.0",
		"@prisma/extension-accelerate": "^2.0.2",
		"@turf/turf": "^7.2.0",
		"@vis.gl/react-google-maps": "^1.6.1",
		"antd": "^5.27.3",
		"better-sqlite3": "^12.4.1",
		"dompurify": "^3.2.7",
		"dotenv": "^17.2.3",
		"jsdom": "^27.1.0",
		"jsonwebtoken": "^9.0.2",
		"maplibre-gl": "^5.6.0",
		"mysql2": "^3.14.5",
		"next": "15.3.4",
		"next-auth": "5.0.0-beta.29",
		"openai": "^5.23.0",
		"pdf-parse-new": "^1.4.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-icons": "^5.5.0",
		"stream-chain": "^3.4.0",
		"stream-json": "^1.9.1",
		"tesseract.js": "^6.0.1",
		"three": "^0.178.0",
		"unstorage": "^1.17.1",
		"validator": "^13.15.15",
		"zod": "^4.1.11"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.4",
		"@faker-js/faker": "^10.0.0",
		"@tailwindcss/postcss": "^4",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/google.maps": "^3.58.1",
		"@types/jsdom": "^27.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^24.10.0",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/stream-json": "^1.7.8",
		"@types/three": "^0.177.0",
		"@vitejs/plugin-react": "^5.0.4",
		"daisyui": "^5.0.46",
		"prisma": "^6.18.0",
		"tailwindcss": "^4",
		"typescript": "^5",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	}
}
